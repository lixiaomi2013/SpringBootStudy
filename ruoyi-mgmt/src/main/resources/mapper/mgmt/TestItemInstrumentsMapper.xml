<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.mgmt.mapper.TestItemInstrumentsMapper">
    
    <resultMap type="TestItemInstruments" id="TestItemInstrumentsResult">
        <result property="id"    column="id"    />
        <result property="testItemId"    column="test_item_id"    />
        <result property="testItemName"    column="test_item_name"    />
        <result property="instrumentId"    column="instrument_id"    />
        <result property="instrumentName"    column="instrument_name"    />
        <result property="sampleType"    column="sample_type"    />
        <result property="price"    column="price"    />
        <result property="isPrimary"    column="is_primary"    />
        <result property="isDefault"    column="is_default"    />
        <result property="sortOrder"    column="sort_order"    />
        <result property="status"    column="status"    />
        <result property="delFlag"    column="del_flag"    />
        <result property="createBy"    column="create_by"    />
        <result property="createTime"    column="create_time"    />
        <result property="updateBy"    column="update_by"    />
        <result property="updateTime"    column="update_time"    />
        <result property="remark"    column="remark"    />
    </resultMap>

    <sql id="selectTestItemInstrumentsVo">
        select id, test_item_id, test_item_name, instrument_id, instrument_name, sample_type, price, is_primary, is_default, sort_order, status, del_flag, create_by, create_time, update_by, update_time, remark from t_mgmt_test_item_instruments
    </sql>

    <select id="selectTestItemInstrumentsList" parameterType="TestItemInstruments" resultMap="TestItemInstrumentsResult">
        <include refid="selectTestItemInstrumentsVo"/>
        <where>  
            <if test="testItemId != null  and testItemId != ''"> and test_item_id = #{testItemId}</if>
            <if test="testItemName != null  and testItemName != ''"> and test_item_name like concat('%', #{testItemName}, '%')</if>
            <if test="instrumentId != null  and instrumentId != ''"> and instrument_id = #{instrumentId}</if>
            <if test="instrumentName != null  and instrumentName != ''"> and instrument_name like concat('%', #{instrumentName}, '%')</if>
            <if test="sampleType != null  and sampleType != ''"> and sample_type = #{sampleType}</if>
            <if test="price != null "> and price = #{price}</if>
            <if test="isPrimary != null  and isPrimary != ''"> and is_primary = #{isPrimary}</if>
            <if test="isDefault != null  and isDefault != ''"> and is_default = #{isDefault}</if>
            <if test="sortOrder != null "> and sort_order = #{sortOrder}</if>
            <if test="status != null  and status != ''"> and status = #{status}</if>
        </where>
    </select>
    
    <select id="selectTestItemInstrumentsById" parameterType="String" resultMap="TestItemInstrumentsResult">
        <include refid="selectTestItemInstrumentsVo"/>
        where id = #{id}
    </select>

    <insert id="insertTestItemInstruments" parameterType="TestItemInstruments">
        insert into t_mgmt_test_item_instruments
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="id != null">id,</if>
            <if test="testItemId != null and testItemId != ''">test_item_id,</if>
            <if test="testItemName != null">test_item_name,</if>
            <if test="instrumentId != null and instrumentId != ''">instrument_id,</if>
            <if test="instrumentName != null">instrument_name,</if>
            <if test="sampleType != null">sample_type,</if>
            <if test="price != null">price,</if>
            <if test="isPrimary != null">is_primary,</if>
            <if test="isDefault != null">is_default,</if>
            <if test="sortOrder != null">sort_order,</if>
            <if test="status != null">status,</if>
            <if test="delFlag != null">del_flag,</if>
            <if test="createBy != null">create_by,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateBy != null">update_by,</if>
            <if test="updateTime != null">update_time,</if>
            <if test="remark != null">remark,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="id != null">#{id},</if>
            <if test="testItemId != null and testItemId != ''">#{testItemId},</if>
            <if test="testItemName != null">#{testItemName},</if>
            <if test="instrumentId != null and instrumentId != ''">#{instrumentId},</if>
            <if test="instrumentName != null">#{instrumentName},</if>
            <if test="sampleType != null">#{sampleType},</if>
            <if test="price != null">#{price},</if>
            <if test="isPrimary != null">#{isPrimary},</if>
            <if test="isDefault != null">#{isDefault},</if>
            <if test="sortOrder != null">#{sortOrder},</if>
            <if test="status != null">#{status},</if>
            <if test="delFlag != null">#{delFlag},</if>
            <if test="createBy != null">#{createBy},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateBy != null">#{updateBy},</if>
            <if test="updateTime != null">#{updateTime},</if>
            <if test="remark != null">#{remark},</if>
         </trim>
    </insert>

    <update id="updateTestItemInstruments" parameterType="TestItemInstruments">
        update t_mgmt_test_item_instruments
        <trim prefix="SET" suffixOverrides=",">
            <if test="testItemId != null and testItemId != ''">test_item_id = #{testItemId},</if>
            <if test="testItemName != null">test_item_name = #{testItemName},</if>
            <if test="instrumentId != null and instrumentId != ''">instrument_id = #{instrumentId},</if>
            <if test="instrumentName != null">instrument_name = #{instrumentName},</if>
            <if test="sampleType != null">sample_type = #{sampleType},</if>
            <if test="price != null">price = #{price},</if>
            <if test="isPrimary != null">is_primary = #{isPrimary},</if>
            <if test="isDefault != null">is_default = #{isDefault},</if>
            <if test="sortOrder != null">sort_order = #{sortOrder},</if>
            <if test="status != null">status = #{status},</if>
            <if test="delFlag != null">del_flag = #{delFlag},</if>
            <if test="createBy != null">create_by = #{createBy},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="updateBy != null">update_by = #{updateBy},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
            <if test="remark != null">remark = #{remark},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteTestItemInstrumentsById" parameterType="String">
        delete from t_mgmt_test_item_instruments where id = #{id}
    </delete>

    <delete id="deleteTestItemInstrumentsByIds" parameterType="String">
        delete from t_mgmt_test_item_instruments where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>

</mapper>